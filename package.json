{
  "name": "product",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rm -Rf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:gen": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "gql:ts:gen": "ts-node src/common/graphql/generate-typing.ts",
    "proto:ts:gen": "tsproto --path common/proto/server --output src/types/grpc"
  },
  "dependencies": {
    "@apollo/subgraph": "2.1.4",
    "@fastify/static": "^5.0.2",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/graphql": "10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/platform-fastify": "^8.4.7",
    "@nestjs/serve-static": "^3.0.0",
    "@prisma/client": "^4.6.1",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "apollo-server-fastify": "^3.11.1",
    "apollo-upload-server": "^7.1.0",
    "await-to-js": "^3.0.0",
    "cache-manager-redis-store": "^2.0.0",
    "cache-manager": "^5.1.1",
    "redis": "^4.3.1",
    "fastify": "^3.0.0",
    "fastify-gql-upload": "^1.0.0",
    "fastify-multer": "^2.0.3",
    "fastify-multipart": "^5.4.0",
    "graphql": "^16.6.0",
    "graphql-upload": "^12.0.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "npm": "^9.1.3",
    "prisma": "^4.6.1",
    "product": "link:",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stream": "^0.0.2",
    "ts-morph": "^17.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "9.4.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
